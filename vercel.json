{
    "version": 2,
    "builds": [
        {
            "src": "movie_review_api/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "movie_review_api/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "movie_review_api.settings"
    }
}
